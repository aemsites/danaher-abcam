                    "template": {
                      "jcr:title": "Terms and conditions",
                      "fieldType": "panel",
                      "fd:viewType": "tnc",
                      "enabled": true,
                      "visible": true,
                      "text": {
                        "value": "Text related to the terms and conditions come here.",
                        "sling:resourceType": "core/fd/components/form/text/v1/text",
                        "fieldType": "plain-text",
                        "textIsRich": true
                      },
                      "link": {
                        "enum": [
